<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <size
                android:width="112dp"
                android:height="84dp" />

            <stroke
                android:width="2dp"
                android:color="@color/grey"
                android:dashWidth="8dp"
                android:dashGap="4dp" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/vicon_add_circle"
        android:gravity="center" />
</layer-list>
